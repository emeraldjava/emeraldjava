{
  "name": "emeraldjava",
  "version": "1.0.0",
  "description": "The CV of emeraldjava",
  "main": "index.js",
  "scripts": {
    "target": "mkdir -p target",
    "rocketspacer": "node ./node_modules/kiss-my-resume/cli.js build ./src/main/resources/json/resume/jsonresume.json -f all -o ./target/rocketspacer -n index -t rocketspacer",
    "classy": "node ./node_modules/kiss-my-resume/cli.js build ./src/main/resources/json/resume/jsonresume.json -f all -o ./target/classy -n index -t classy",
    "caffeine": "node ./node_modules/kiss-my-resume/cli.js build ./src/main/resources/json/resume/jsonresume.json -f all -o ./target -n index -t caffeine",
    "clean": "del-cli --force ./target",
    "all": "npm-run-all -p -r rocketspacer classy caffeine",
    "deploy": "gh-pages -d target -r https://$GITHUB_TOKEN@github.com/emeraldjava/emeraldjava.git -u \"emeraldjava <paul.t.oconnell@gmail.com>\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emeraldjava/emeraldjava.git"
  },
  "author": "emeraldjava",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emeraldjava/emeraldjava/issues"
  },
  "homepage": "https://github.com/emeraldjava/emeraldjava#readme",
  "dependencies": {
    "jsonresume-theme-caffeine": "^1.2.3",
    "jsonresume-theme-classy": "^1.0.9",
    "jsonresume-theme-elegant": "^1.7.11",
    "jsonresume-theme-material": "^1.1.3",
    "jsonresume-theme-modern": "^0.0.5",
    "jsonresume-theme-onepage": "0.0.3",
    "jsonresume-theme-rocketspacer": "^1.2.2",
    "jsonresume-theme-short": "^0.1.7",
    "jsonresume-theme-stackoverflow": "^1.4.0",
    "kiss-my-resume": "^0.8.2"
  },
  "devDependencies": {
    "del-cli": "^4.0.1",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5"
  }
}
